{
  "name": "v2",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "build-storybook",
    "dev": "vite",
    "prepare": "husky install",
    "preview": "vite preview",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "storybook": "start-storybook -p 6006",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "update:changelog": "node scripts/updateChangelog.js"
  },
  "lint-staged": {
    "CHANGELOG.md": "npm run update:changelog",
    "package.json": "npx sort-package-json",
    "*.{ts,tsx,js,jsx,css,md}": "prettier --write",
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*": "prettier --ignore-unknown --write"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "classnames": "^2.3.1",
    "cross-fetch": "^3.1.5",
    "firebase": "^9.6.11",
    "framer-motion": "^6.3.0",
    "ramda": "^0.28.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-firebase-hooks": "^5.0.3",
    "react-flow-renderer": "^10.1.1",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-controls": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-toolbars": "^6.4.22",
    "@storybook/builder-vite": "^0.1.29",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/ramda": "^0.28.7",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-helmet": "^6.1.5",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitest/ui": "^0.9.4",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "eslint": "^8.12.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.5",
    "msw": "^0.39.2",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vitest": "^0.9.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
